{
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.pnpm-lock.json",
    ".vscode",
    "megalinter",
    "megalinter-reports/**",
    "coverage/**",
    "coverage_nyc/**",
    "package-lock.json",
    "report",
    ".*"
  ],
  "ignoreWords": [
    "niek",
    "npalm",
    "nvmrc",
    "markdownlintignore",
    "devcontainer",
    "vitest"
  ],
  "language": "en",
  "noConfigSearch": true,
  "words": ["linebreak", "megalinter", "oxsecurity", "REAMDE"],
  "version": "0.2"
}
